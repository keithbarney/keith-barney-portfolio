    //- Band filter script
    script.
      (function () {
        var buttons = document.querySelectorAll('.band-filter');
        var sections = document.querySelectorAll('.band-section');
        buttons.forEach(function (btn) {
          btn.addEventListener('click', function () {
            var filter = btn.getAttribute('data-filter');
            buttons.forEach(function (b) { b.classList.remove('band-filter--active'); });
            btn.classList.add('band-filter--active');
            sections.forEach(function (s) {
              s.style.display = filter === 'all' || s.getAttribute('data-band') === filter ? '' : 'none';
            });
          });
        });
      })();

