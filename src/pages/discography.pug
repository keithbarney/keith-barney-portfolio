doctype html
html(lang="en" data-theme="dark")
  - var title = "Discography"
  - var description = "Keith Barney â€” Complete discography across Adamantium, Throwdown, Death by Stereo, Eighteen Visions, and Duhkha"
  include partials/_head

  body
    //- Header
    header.header
      .container.flex.justify-between.items-center
        a.wordmark(href="/") Keith Barney
        nav.flex.heavy-gap-3
          a.nav-link.nav-link--active(href="/discography.html") Discography
          a.nav-link(href="mailto:hello@keithbarney.com") Contact
          button#theme-toggle(aria-label="Toggle theme") Light

    main.discography

      //- Hero
      section.disco-hero
        .container
          .heavy-grid.heavy-grid--12
            .heavy-col-span-8
              h1.disco-title Discography
              p.disco-subtitle 30 years of records across 5 bands. Every pressing, every variant.

      //- Summary Stats
      section.disco-stats.disco-stats--top
        .container
          .heavy-grid.heavy-grid--12
            .heavy-col-span-full
              .stats-grid
                .stat
                  span.stat-value 5
                  span.stat-label Bands
                .stat
                  span.stat-value ~27
                  span.stat-label Releases
                .stat
                  span.stat-value 10
                  span.stat-label Studio Albums
                .stat
                  span.stat-value 3
                  span.stat-label Re-recorded Albums
                .stat
                  span.stat-value 12+
                  span.stat-label Compilations
                .stat
                  span.stat-value 11+
                  span.stat-label Labels
                .stat
                  span.stat-value 30
                  span.stat-label Years Active

      //- Band Filters
      section.band-filters
        .container
          .heavy-grid.heavy-grid--12
            .heavy-col-span-full
              .band-filters-row
                button.band-filter.band-filter--active(data-filter="all") All
                button.band-filter(data-filter="adamantium") Adamantium
                button.band-filter(data-filter="throwdown") Throwdown
                button.band-filter(data-filter="death-by-stereo") Death by Stereo
                button.band-filter(data-filter="eighteen-visions") Eighteen Visions
                button.band-filter(data-filter="duhkha") Duhkha

